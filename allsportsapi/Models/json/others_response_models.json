    "AbstractNetworkResponse": {
      "description": "Base response class for legacy endpoints",
      "fields": {
        "head": {
          "type": "HeadResponse",
          "nullable": true,
          "description": "Response metadata including status and timing"
        },
        "error": {
          "type": "HeadResponse",
          "nullable": true,
          "description": "Error information if request failed"
        }
      }
    },
